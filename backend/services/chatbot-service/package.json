{
  "name": "@hospital/chatbot-service",
  "version": "1.0.0",
  "description": "Hospital AI Chatbot Service with Health Advisory Features",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "node -r ts-node/register src/index.ts",
    "dev:simple": "node simple-server.js",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.49.8",
    "axios": "^1.10.0",
    "compromise": "^14.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "natural": "^6.12.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.0"
  },
  "keywords": [
    "hospital",
    "chatbot",
    "health-advisor",
    "healthcare",
    "ai",
    "vietnamese",
    "symptom-analysis",
    "medical-consultation"
  ],
  "author": "Hospital Management System",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
